<odoo>
    <record id="seq_hr_employee_num" model="ir.sequence">
        <field name="name">员工工号</field>
        <field name="code">hr.employee.num</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_hr_department_num" model="ir.sequence">
        <field name="name">部门编码</field>
        <field name="code">hr.department.num</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>