<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ui_workflow_route_view_form" model="ir.ui.view">
    	<field name="name">路线</field>
    	<field name="model">oa.workflow.route</field>
    	<field name="priority" eval="16"/>
    	<field name="arch" type="xml">
    		<form string="">
				<sheet>
					<group>
						<field name="name"/>
						<field name="model_name"/>
						<field name="stage_ids"/>
						<field name="sequence"/>
					</group>
				</sheet>
    		</form>
    	</field>
    </record>

    <record id="ui_workflow_route_action" model="ir.actions.act_window">
    	<field name="name">创建路线</field>
    	<field name="res_model">oa.workflow.route</field>
    	<field name="view_type">form</field>
    	<field name="view_mode">tree,form</field>
    </record>

</odoo>