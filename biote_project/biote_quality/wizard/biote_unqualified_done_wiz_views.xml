<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- biote.unqualified.done.wiz form view -->
    <record id="biote_unqualified_done_wiz_view_form" model="ir.ui.view">
        <field name="name">biote.unqualified.done.wiz.view.form</field>
        <field name="model">biote.unqualified.done.wiz</field>
        <field name="arch" type="xml">
            <form string="不合格处置单完成向导">
                <sheet>
                    <group>
                        <field name="unqualified_id" invisible="1"/>
                        <field name="can_handle_qty" readonly="1"/>
                        <field name="handle_qty" required="1"/>
                        <field name="handle_id" required="1"/>
                    </group>
                    <footer>
                        <button name="action_done" string="执行" type="object" class="oe_highlight"/>
                        <button string="取消" class="btn-default" special="cancel"/>
                    </footer>
                </sheet>
            </form>
        </field>
    </record>
</odoo>